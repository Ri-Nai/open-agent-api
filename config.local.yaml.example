# 本地配置文件示例
# 请将此文件重命名为 config.local.yaml 并填入真实的配置值

# Agent API 服务配置
agent:
  api_base_url: "https://agent.bit.edu.cn"
  app_id: "app_id"  # 请替换为您的真实APP ID
  api_key: "api_key"  # 请替换为您的真实API KEY

# 服务器配置
server:
  host: "0.0.0.0"
  port: 8000
  auth_key: ""  # 可选：设置API认证密钥

# 会话管理配置
session:
  max_conversations: 1000
  timeout: 3600

# 日志配置
logging:
  level: "INFO"
  verbose: false 
