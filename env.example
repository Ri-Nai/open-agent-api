# Agent API 环境变量配置示例
# 请复制此文件为 .env 并填入真实的配置值

# Agent API 配置（必需）
AGENT_API_BASE_URL=https://agent.bit.edu.cn
AGENT_APP_ID=your_app_id_here
AGENT_API_KEY=your_api_key_here

# 服务器配置
SERVER_HOST=0.0.0.0
SERVER_PORT=8000
API_AUTH_KEY=

# 会话管理配置
MAX_CONVERSATIONS=1000
CONVERSATION_TIMEOUT=3600

# 日志配置
LOG_LEVEL=INFO
VERBOSE_LOGGING=false 
